//VIDEO 17 EJERCICIO 2

Proceso principal

//definir variables
  definir a,b como reales;
	definir resultado como logico;
	//entrada 
	escribir "dijite el valor de a:";
	leer a;
	escribir "dijite el valor de b:";
	leer b;
	//proceso
	resultado <- (3+5*8)<3 y((-6/3*4)+2<2) o (a>b);
	//salida
	escribir "El resultado es:",resultado;
FinProceso
