//VIDEO 20 EJERCICIO 5

Proceso programa1
	// definir variables
	definir a,b,resultado como entero;
	a <- 10;
	Escribir "dijite un numero: ";
	Leer b;
  
	resultado <- a+b;
  
	Escribir "el resultado es: ",resultado;
  
FinProceso
