//VIDEO 18 EJERCICIO 3
//ejercicio 3: intercambiar el valor de 2 variables

Proceso sin_titulo

//definir variables
	definir a,b,aux Como Entero;
  
	//entrada
	Escribir "dijite el valor de a:";
	leer a;
	escribir "dijite el valor de b:";
	leer b;
  
	//proceso
	aux <- a;
	a <- b;
	b <- aux;
  
	//salida
	Escribir "el nuevo valor de a es:",a;
	Escribir "el nuevo valor de b es:",b;
	
FinProceso
-----------------------------------------------------------
//video 19 ejercico 4

Proceso programa1

	// definir variables
	definir a,b,c,resultado como entero;
	a <- 10;
	b <- 20;
  
  //entrada
	Escribir "dijite un numero: ";
	Leer c;
  
  //proceso
	resultado <- a+b+c;
  
  //salida
	Escribir "el resultado es: ",resultado;
FinProceso
